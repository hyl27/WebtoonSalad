<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.api.mapper.WebtoonMapper">
  <update id="updateLastup" parameterType="String">
    UPDATE tbl_webtoon
    SET lastup = SYSDATE
    WHERE id = #{id}
  </update>
</mapper>